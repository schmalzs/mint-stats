    "lint": "eslint .",
    "start": "node dist/index.js",
    "start:dev": "nodemon src/index.js --exec babel-node",
    "test": "mocha -r chai/register-expect --compilers js:@babel/register",
    "test:watch": "nodemon --exec npm test"
